@model SKSLearningSystem.Models.ViewModels.AdminViewModels.AssignCourseToUsersViewModel
    @using (Html.BeginForm("finalassign","admin",FormMethod.Post,new { @class="form-assign"}))
    {
        <div class="user-name">Username</div>
        <div class="user-checked">Select</div>
        <div class="user-mand">Mandatory</div>
        <div class="user-duedate">DueDate</div>
        <div class="filler">Grade</div>
        

        for (int i = 0; i < Model.Users.Count; i++)
        {
            @Html.HiddenFor(x => x.CourseId);
            @Html.HiddenFor(x => x.Users[i].Id);
            @Html.DisplayFor(x => x.Users[i].UserName, new { @class="user-name" });
            @Html.EditorFor(x => x.Users[i].Checked, new { @class = "user-checked" });
             @Html.EditorFor(x => x.Users[i].Mandatory, new { @class = "user-mand" })
             @Html.EditorFor(x => x.Users[i].DueDate, new { @class = "user-duedate" })
            @Html.EditorFor(x => x.Users[i].Grade, new { @class = "filler" })
        }
        <button type="submit"> Assign</button>
    }
