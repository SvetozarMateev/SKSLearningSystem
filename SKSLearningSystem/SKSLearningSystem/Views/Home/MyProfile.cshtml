@model SKSLearningSystem.Areas.Admin.Models.UserViewModel

<div id="containerUserProfile">
    <div id="profilePicture">
        <img src="smiley.gif" alt="Smiley face" height="42" width="42">
    </div>
    <div id="personalData">
        <p id="userName">@Model.UserName</p>
    </div>
    <div id="userCourses">
        @for (int i = 0; i < Model.CourseStates.Count; i++)
        {
            <p>@Model.CourseStates[i].CourseName</p>
        }
    </div>
</div>