@model List<SKSLearningSystem.Models.ViewModels.SingleCourseViewModel>

<section class="recipe-recommend">
    <div class="wrapper-scroller-menu slide">
        @for (int i = 0; i < Model.Count; i++)
        {
            <article class="recipe" index="0">
                @Html.Partial("_SingleCourse", Model[i])
            </article>
        }
    </div>
</section>